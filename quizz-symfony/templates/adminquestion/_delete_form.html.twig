<form method="post" action="{{ path('app_adminquestion_delete', {'id': question.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ question.id) }}">
    <button class="btn btn-danger btn-lg mt-5">Delete</button>
</form>
